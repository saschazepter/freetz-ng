config EXTERNAL_FREETZ_PACKAGE_PHP5
	depends on EXTERNAL_ENABLED && FREETZ_PACKAGE_PHP5
	bool "PHP5"
	default n

if EXTERNAL_FREETZ_PACKAGE_PHP5

config EXTERNAL_FREETZ_PACKAGE_PHP5_cgi
	depends on FREETZ_PACKAGE_PHP5_cgi
	bool "PHP CGI"
	default y
	help
		externals the following file(s):
		 /usr/bin/php-cgi

config EXTERNAL_FREETZ_PACKAGE_PHP5_cli
	depends on FREETZ_PACKAGE_PHP5_cli
	bool "PHP CLI"
	default y
	help
		externals the following file(s):
		 /usr/bin/php

config EXTERNAL_FREETZ_PACKAGE_PHP5_apxs2
	depends on FREETZ_PACKAGE_PHP5_apxs2
	bool "Apache2 module"
	default y
	help
		externals the following file(s):
		 /usr/lib/apache2/libphp5.so

endif # EXTERNAL_FREETZ_PACKAGE_PHP5

